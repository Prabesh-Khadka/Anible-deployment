server {        
   listen 80;        
   listen [::]:80;  
           
   #server_name gibllivebanking.gibl.com.np;    
   
  location / {
  
        root /var/www/html;
        #return 301 https://$host$request_uri;
	index index.html index.htm index.nginx-debian.html;
        try_files $uri $uri/ =404; 

}
      
   location /mikha/ {             
           proxy_pass http://127.0.0.1:8080/;
}
}





